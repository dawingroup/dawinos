    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "firebase": "^10.7.1",
    "framer-motion": "^12.23.26",
    "fuse.js": "^7.1.0",
    "googleapis": "^131.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.263.1",
