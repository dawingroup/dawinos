{
  "name": "dawinos-finishes",
  "version": "2.0.0",
  "description": "DawinOS - Dawin Finishes: Manufacturing & millwork operations platform for the Dawin Group",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "npm run build && firebase deploy",
    "deploy:preview": "npm run build && firebase hosting:channel:deploy preview",
    "firebase:login": "firebase login",
    "firebase:init": "firebase init hosting",
    "seed:design": "npx ts-node --esm scripts/seed-design-manager.ts",
    "generate:design-events": "node scripts/generate-design-manager-events.cjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config src/testing/config/vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:migration": "vitest run --config vitest.migration.config.ts",
    "test:performance": "vitest run --config src/testing/config/vitest.config.ts --dir src/testing/performance",
    "test:benchmark": "vitest bench",
    "test:e2e": "playwright test --config e2e/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --config e2e/config/playwright.config.ts --ui",
    "emulators": "firebase emulators:start",
    "emulators:test": "firebase emulators:exec --only firestore,auth 'npm run test:integration'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
